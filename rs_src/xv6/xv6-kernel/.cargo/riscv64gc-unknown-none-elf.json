{
  "arch": "riscv64",
  "code-model": "medium",
  "cpu": "generic-rv64",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n32:64-S128",
  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "features": "+m,+a,+f,+d,+c",
  "is-builtin": false,
  "llvm-abiname": "lp64d",
  "llvm-target": "riscv64",
  "max-atomic-width": 64,
  "panic-strategy": "abort",
  "relocation-model": "static",
  "target-pointer-width": "64",

  "linker": "rust-lld",
  "linker-flavor": "ld.lld",
  "pre-link-args": {
    "ld.lld": [
      "-z",
      "max-page-size=4096",
      "-T",
      "../../../kernel/kernel.ld",
      "-o",
      "../../../kernel/kernel",
      "../../../kernel/entry.o",
      "../../../kernel/start.o",
      "../../../kernel/console.o",
      "../../../kernel/printf.o",
      "../../../kernel/uart.o",
      "../../../kernel/kalloc.o",
      "../../../kernel/spinlock.o",
      "../../../kernel/string.o",
      "../../../kernel/main.o",
      "../../../kernel/vm.o",
      "../../../kernel/proc.o",
      "../../../kernel/swtch.o",
      "../../../kernel/trampoline.o",
      "../../../kernel/trap.o",
      "../../../kernel/syscall.o",
      "../../../kernel/sysproc.o",
      "../../../kernel/bio.o",
      "../../../kernel/fs.o",
      "../../../kernel/log.o",
      "../../../kernel/sleeplock.o",
      "../../../kernel/file.o",
      "../../../kernel/pipe.o",
      "../../../kernel/exec.o",
      "../../../kernel/sysfile.o",
      "../../../kernel/kernelvec.o",
      "../../../kernel/plic.o",
      "../../../kernel/virtio_disk.o"
    ]
  }
}
